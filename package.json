{
  "name": "@examples/trpc-next-typegoose-starter",
  "version": "10.35.0",
  "private": true,
  "scripts": {
    "dx:next": "run-s migrate && next dev",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "migrate": "echo \"No migrations to run\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "test": "run-s test:*",
    "test:ci": "jest --ci",
    "test:unit": "jest",
    "test:watch": "jest --watch --test-timeout 10000",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "pnpm dx"
  },
  "packageManager": "pnpm@8.5.0",
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.35.0",
    "@trpc/next": "^10.35.0",
    "@trpc/react-query": "^10.35.0",
    "@trpc/server": "^10.35.0",
    "@typegoose/typegoose": "^11.4.0",
    "clsx": "^2.0.0",
    "mongodb": "^5.7.0",
    "mongoose": "^7.4.1",
    "next": "^13.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.12.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@swc/core": "^1.3.73",
    "@swc/jest": "^0.2.27",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-environment-node": "^29.6.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.8.8",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
